{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.hbs$",
                "cmd": "touch -m .watch"
            },
            {
                "match": "\\.json$",
                "cmd": "touch -m .watch"
            },
            {
                "match": "\\.scss$",
                "cmd": "touch -m .watch"
            },
            {
                "match": "\\.ts$",
                "cmd": "touch -m .watch"
            }
        ]
    },
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.exclude": {
        ".cache": true,
        ".config": true,
        ".npm": true,
        ".nyc_output": true,
        ".stryker-tmp": true,
        ".vscode-server": true,
        ".bash_history": true,
        ".bash_logout": true,
        ".bashrc": true,
        ".HEAD": true,
        ".profile": true,
        ".screenrc": true,
        ".watch": true,
        "cucumber.js": true,
        "dependencyGraph.json": true,
        "package-lock.json": true,
        "tsconfig.json": true
    },
    "files.watcherExclude": {
        ".cache/**": true,
        ".config/**": true,
        ".npm/**": true,
        ".stryker-tmp/**": true,
        ".vscode/**": true,
        ".vscode-server/**": true,
        "node_modules/**": true,
        "app/output/**": true,
        "app/pattern_exports/**": true,
        "app/www/**": true,
        ".bash_logout": true,
        ".bashrc": true,
        ".profile": true,
        ".screenrc": true,
        "cucumber.js": true,
        "dependencyGraph.json": true,
        "package-lock.json": true,
        "tsconfig.json": true
    },
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "npm.exclude": [
        "**/gitRepo/**"
    ],
    "npm.scriptExplorerAction": "run"
}
