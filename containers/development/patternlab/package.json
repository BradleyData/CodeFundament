{
    "name": "patternlab",
    "version": "0.0.0",
    "scripts": {
        "compile": "npm run pl:build; npm run sass",
        "patternlab": "patternlab",
        "pl:build": "patternlab build --config ./patternlab-config.json",
        "sass": "find app/source/ -name '*.scss' -exec cat {} \\; | npx sass - app/public/css/style.css"
    },
    "main": "index.js",
    "dependencies": {
    },
    "devDependencies": {
        "@pattern-lab/cli": "5.11.1",
        "@pattern-lab/core": "5.11.1",
        "@pattern-lab/engine-handlebars": "5.10.1",
        "@pattern-lab/plugin-tab": "5.10.1",
        "@pattern-lab/uikit-workshop": "5.11.1",
        "browser-sync": "2.26.12",
        "gulp": "4.0.2",
        "node-sass": "4.14.1"
    }
}