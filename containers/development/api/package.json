{
    "name": "api",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "compile": "babel app/src/ --out-dir app/www/ --extensions '.ts'",
        "document": "typedoc",
        "lint": "prettier --write 'app/acceptanceTests/steps/**/**' 'app/src/**/**' && eslint --fix 'app/acceptanceTests/steps/**/**' 'app/src/**/**' && echo 'All files passed!'",
        "unitTests": "jest --detectOpenHandles",
        "mutationTests": "stryker run",
        "acceptanceTests": "cucumber-js; node cucumberHtmlReporter.mjs"
    },
    "main": "app/www/index.js",
    "dependencies": {},
    "devDependencies": {
        "@babel/cli": "7.10.5",
        "@babel/core": "7.10.5",
        "@babel/node": "7.10.5",
        "@babel/plugin-proposal-class-properties": "7.10.4",
        "@babel/plugin-proposal-object-rest-spread": "7.10.4",
        "@babel/preset-typescript": "7.10.4",
        "@stryker-mutator/core": "3.3.1",
        "@stryker-mutator/jest-runner": "3.3.1",
        "@stryker-mutator/typescript": "3.3.1",
        "@types/cucumber": "4.0.4",
        "@types/jest": "26.0.7",
        "@types/node": "14.0.26",
        "@typescript-eslint/eslint-plugin": "3.7.0",
        "@typescript-eslint/parser": "3.7.0",
        "cucumber": "6.0.5",
        "cucumber-html-reporter": "5.2.0",
        "cucumber-pretty": "6.0.0",
        "eslint": "7.5.0",
        "eslint-plugin-prettier": "3.1.4",
        "jest": "26.1.0",
        "prettier": "2.0.5",
        "ts-jest": "26.1.3",
        "ts-loader": "8.0.1",
        "ts-node-dev": "1.0.0-pre.56",
        "typedoc": "0.17.8",
        "typescript": "3.9.7"
    }
}
